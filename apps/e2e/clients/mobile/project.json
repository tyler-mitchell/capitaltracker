{
    "name": "e2e-mobile",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/e2e/clients/mobile/src",
    "projectType": "application",
    "targets": {
        "build-ios": {
            "executor": "@nrwl/detox:build",
            "options": {
                "detoxConfiguration": "ios.sim.debug"
            },
            "configurations": {
                "production": {
                    "detoxConfiguration": "ios.sim.release"
                }
            }
        },
        "test-ios": {
            "executor": "@nrwl/detox:test",
            "options": {
                "detoxConfiguration": "ios.sim.debug",
                "buildTarget": "client-e2e-mobile:build-ios"
            },
            "configurations": {
                "production": {
                    "detoxConfiguration": "ios.sim.release",
                    "buildTarget": "client-e2e-mobile:build-ios:production"
                }
            }
        },
        "build-android": {
            "executor": "@nrwl/detox:build",
            "options": {
                "detoxConfiguration": "android.emu.debug"
            },
            "configurations": {
                "production": {
                    "detoxConfiguration": "android.emu.release"
                }
            }
        },
        "test-android": {
            "executor": "@nrwl/detox:test",
            "options": {
                "detoxConfiguration": "android.emu.debug",
                "buildTarget": "client-e2e-mobile:build-ios"
            },
            "configurations": {
                "production": {
                    "detoxConfiguration": "android.emu.release",
                    "buildTarget": "client-e2e-mobile:build-ios:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "apps/e2e/clients/mobile/**/*.{ts,tsx,js,jsx}"
                ]
            }
        }
    },
    "tags": [],
    "implicitDependencies": ["mobile"]
}
