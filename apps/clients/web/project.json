{
    "name": "web",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/clients/web",
    "projectType": "application",
    "namedInputs": {
        "defaultBump": ["default", "noPackageJson"]
    },
    "targets": {
        "start": {
            "inputs": ["default"],
            "executor": "@nrwl/next:server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "web:build",
                "dev": true
            },
            "configurations": {
                "development": {
                    "buildTarget": "web:build:development",
                    "dev": true
                },
                "production": {
                    "buildTarget": "web:build:production",
                    "dev": false
                }
            }
        },
        "build": {
            "inputs": ["default"],
            "executor": "@nrwl/next:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "root": "apps/clients/web",
                "outputPath": "dist/apps/clients/web",
                "postcssConfig": "apps/clients/web/postcss.config.js"
            },
            "configurations": {
                "development": {
                    "outputPath": "dist/apps/clients/web"
                },
                "production": {}
            }
        },
        "export": {
            "inputs": ["default"],
            "dependsOn": ["build"],
            "executor": "@nrwl/next:export",
            "options": {
                "buildTarget": "web:build:production"
            }
        },
        "test": {
            "inputs": ["default"],
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/clients/web/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "inputs": ["default"],
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/clients/web/**/*.{ts,tsx,js,jsx}"]
            }
        }
    },
    "tags": []
}
