{
    "name": "node-api-wrapper",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/api/node-api-wrapper/src",
    "projectType": "library",
    "targets": {
        "build": {
            "inputs": ["default"],
            "dependsOn": ["^build"],
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/api/node-api-wrapper"
            }
        },
        "lint": {
            "inputs": ["default"],
            "dependsOn": ["^build"],
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/api/node-api-wrapper/**/*.ts"]
            }
        },
        "test": {
            "inputs": ["default"],
            "dependsOn": ["^build"],
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/api/node-api-wrapper/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "implicitDependencies": ["node-api-models", "api"],
    "tags": []
}
