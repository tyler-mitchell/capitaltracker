{
    "name": "rust-api-models",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/api/models/rust-api-models/src",
    "namedInputs": {
        "default": ["{projectRoot}/**/*"]
    },
    "targets": {
        "build": {
            "inputs": ["grpcModels", "default"],
            "executor": "@ignisda/nx-rust:build",
            "options": {
                "release": false,
                "locked": true
            },
            "configurations": {
                "production": {
                    "release": true,
                    "locked": true
                }
            }
        },
        "test": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "executor": "@ignisda/nx-rust:test",
            "options": {
                "release": false,
                "frozen": true,
                "locked": true
            },
            "configurations": {
                "production": {
                    "release": true,
                    "frozen": true,
                    "locked": true
                }
            }
        },
        "lint": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "executor": "@ignisda/nx-rust:clippy",
            "options": {
                "failOnWarnings": true,
                "fix": false,
                "noDeps": true
            }
        },
        "fmt": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "command": "cargo fmt -p rust-api-models --check"
        }
    },
    "tags": []
}
