{
    "name": "rust-api-models",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/api/models/rust-api-models/src",
    "namedInputs": {
        "default": ["{projectRoot}/**/*"]
    },
    "targets": {
        "build": {
            "inputs": ["grpcModels", "default"],
            "executor": "@ignisda/nx-rust:build",
            "defaultConfiguration": "production",
            "options": {
                "locked": true
            },
            "configurations": {
                "production": {
                    "release": true
                },
                "development": {
                    "release": false
                }
            }
        },
        "test": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "executor": "@ignisda/nx-rust:test",
            "defaultConfiguration": "production",
            "options": {
                "frozen": true,
                "locked": true
            },
            "configurations": {
                "production": {
                    "release": true
                },
                "development": {
                    "release": false
                }
            }
        },
        "lint": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "defaultConfiguration": "production",
            "executor": "nx:run-commands",
            "configurations": {
                "production": {
                    "command": "cargo clippy --release -- --D warnings --no-deps"
                },
                "development": {
                    "command": "cargo clippy -- --D warnings --no-deps"
                }
            }
        },
        "fmt": {
            "dependsOn": ["build"],
            "inputs": ["grpcModels", "default"],
            "command": "cargo fmt -p rust-api-models --check"
        }
    },
    "tags": []
}
