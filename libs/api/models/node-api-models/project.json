{
    "name": "node-api-models",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/api/models/node-api-models/src",
    "projectType": "library",
    "namedInputs": {
        "codegen": ["{workspaceRoot}/tools/scripts/generate-node-api-models.sh"]
    },
    "targets": {
        "codegen": {
            "inputs": ["codegen", "default", "grpcModels"],
            "command": "libs/api/models/node-api-models/codegen.sh"
        },
        "build": {
            "dependsOn": ["codegen"],
            "executor": "@nrwl/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/api/models/node-api-models"
            }
        },
        "lint": {
            "dependsOn": ["codegen"],
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/api/models/node-api-models/**/*.ts"]
            }
        },
        "test": {
            "dependsOn": ["codegen"],
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "libs/api/models/node-api-models/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
